	<queryString language="SQL">
		<![CDATA[-- Laporan Jurnal Sederhana - Hanya dari table journal_entries
SELECT 
    ROW_NUMBER() OVER (ORDER BY je.entry_date, je.entry_number) AS no_urut,
    je.entry_number AS no_jurnal,
    DATE_FORMAT(je.entry_date, '%d/%m/%Y') AS tanggal,
    je.description AS keterangan,
    je.debit_account_id AS kode_debit,
    je.credit_account_id AS kode_kredit,
    FORMAT(je.amount, 0) AS jumlah,
    je.reference_number AS referensi
FROM journal_entries je
WHERE je.entry_date BETWEEN '2025-01-01' AND '2025-12-31'
ORDER BY je.entry_date, je.entry_number;]]>
	</queryString>
